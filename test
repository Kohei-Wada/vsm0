#!/bin/bash


vsm="./vsm"


if ! [ -e "$vsm" ]; then
	echo vsm does not exist.
	exit 1;
fi


while IFS=',' read exp ans 
do 
	retval=$(echo "$exp" | $vsm)
	ans=$(echo "$ans" | cut -d " " -f 2)

	echo -n "retval = $retval, ans = $ans"

	if [ "$retval" -eq "$ans" ] ; then
		echo -e "\033[32m ok \033[m"
	else
		echo -e "\033[31m test failed \033[m" >&2
	fi


done < expressions
